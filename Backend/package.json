{
  "name": "remote-manager",
  "productName": "remote-manager",
  "version": "1.6.5",
  "author": "delg [me@delg.cc]",
  "description": "Remote Manager Application",
  "main": "index.js",
  "scripts": {
    "test": "electron .",
    "make": "electron-builder --win --x64"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.1",
    "express": "^4.21.1",
    "fs-extra": "^11.2.0",
    "open": "^8.4.2"
  },
  "devDependencies": {
    "electron": "^33.0.1",
    "electron-builder": "^25.1.8"
  },
  "build": {
    "appId": "cc.delg.remotemanager",
    "productName": "Remote Manager",
    "win": {
      "target": "nsis"
    },
    "nsis": {
      "oneClick": true,
      "perMachine": true,
      "allowToChangeInstallationDirectory": false
    }
  }
}